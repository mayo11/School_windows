1、寻找数组的中心索引
class Solution {
public:
    int pivotIndex(vector<int>& nums) {
        int i,j,lsum=0,rsum=0,re=-1;
        for(i=1;i<nums.size();i++){
            rsum+=nums[i];
        }
        for(j=0;j<nums.size();j++){
            if(lsum==rsum){
                re=j;
                break;
            }
            lsum+=nums[j];
            rsum-=nums[j+1];
        }
        return re;
    }
};
